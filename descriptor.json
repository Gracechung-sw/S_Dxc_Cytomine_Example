{
    "name": "Dxc_Cytomine_Example",
    "description": "Deepbio Openapi algorithm",
    "container-image": {
        "image": "dockerdeepbiodev/s_dxc_cytomine_example",
        "type": "singularity"
    },
    "schema-version": "cytomine-0.1",
    "command-line": "python example.py CYTOMINE_HOST CYTOMINE_PUBLIC_KEY CYTOMINE_PRIVATE_KEY CYTOMINE_ID_PROJECT CYTOMINE_ID_SOFTWARE AI_MODEL_TYPE",
    "inputs": [
        {
            "id": "cytomine_host",
            "name": "Cytomine host",
            "set-by-server": true,
            "optional": false,
            "type": "String",
            "value-key": "[@ID]",
            "command-line-flag": "--@id"
        },
        {
            "id": "cytomine_public_key",
            "name": "Cytomine public key",
            "set-by-server": true,
            "optional": false,
            "type": "String",
            "value-key": "[@ID]",
            "command-line-flag": "--@id"
        },
        {
            "id": "cytomine_private_key",
            "name": "Cytomine private key",
            "set-by-server": true,
            "optional": false,
            "type": "String",
            "value-key": "[@ID]",
            "command-line-flag": "--@id"
        },
        {
            "id": "cytomine_id_project",
            "name": "Cytomine project ID",
            "set-by-server": true,
            "optional": false,
            "type": "Number",
            "value-key": "[@ID]",
            "command-line-flag": "--@id"
        },
        {
            "id": "cytomine_id_software",
            "name": "Cytomine software ID",
            "set-by-server": true,
            "optional": false,
            "type": "Number",
            "value-key": "[@ID]",
            "command-line-flag": "--@id"
        },
        {
            "id": "ai_model_type",
            "name": "AI model type",
            "description": "Analysis Model type. ProstateResection_0 | ProstateTURP_0 | BreastLymphNode_0 | BreastResection_0",
            "type": "String",
            "optional": false,
            "default-value": "PRAD_GLEASON_SCORE",
            "value-key": "[@ID]",
            "command-line-flag": "--@id"
        }
    ]
}
